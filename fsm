digraph "State Machine" {
	graph [color=black directed=true fillcolor=white label="State Machine" rankdir=LR "strict"=false style=solid]
	node [color=black fillcolor=white peripheries=1 shape=rectangle style="rounded, filled"]
	edge [color=black]
	user [label="user\l" color=red fillcolor=darksalmon peripheries=2]
	state1 [label="state1\l"]
	state2 [label="state2\l"]
	state3 [label="state3\l"]
	state4 [label="state4\l"]
	state5 [label="state5\l"]
	state6 [label="state6\l"]
	state7 [label="state7\l" color=blue fillcolor=azure2 peripheries=1]
	state8 [label="state8\l"]
	user -> state1 [label="advance [is_going_to_state1]"]
	user -> state2 [label="advance [is_going_to_state2]"]
	user -> state3 [label="advance [is_going_to_state3]"]
	user -> state4 [label="advance [is_going_to_state4]"]
	user -> state5 [label="advance [is_going_to_state5]"]
	user -> state6 [label="advance [is_going_to_state6]"]
	user -> state7 [label="advance [is_going_to_state7]"]
	user -> state8 [label="advance [is_going_to_state8]"]
	state1 -> user [label=go_back]
	state2 -> user [label=go_back]
	state3 -> user [label=go_back]
	state4 -> user [label=go_back]
	state5 -> user [label=go_back]
	state6 -> user [label=go_back]
	state7 -> user [label=go_back color=blue]
	state8 -> user [label=go_back]
}
